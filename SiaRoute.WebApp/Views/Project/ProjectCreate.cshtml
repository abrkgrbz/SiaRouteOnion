@{
    ViewData["Title"] = "Yeni Proje Oluştur";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section css {
    <link href="~/assets/custom/projectcreate.css" rel="stylesheet" type="text/css" />
    
}

<!-- Ana Kart -->
<div class="card card-custom gutter-b">
    <!-- Kart Başlığı -->
    <div class="card-header border-0 pt-6">
        <h3 class="card-title align-items-start flex-column">
            <span class="card-label fw-bolder text-dark">Yeni Proje Oluştur</span>
            <span class="text-muted mt-1 fw-bold fs-7">Yeni bir proje oluşturmak için adımları takip edin</span>
        </h3>
        <div class="card-toolbar">
           
            <a href="@Url.Action("ProjectList")" class="btn btn-sm btn-light">
                <i class="bi bi-arrow-left me-1"></i> Listeye Dön
            </a>
        </div>
    </div>

    <!-- Kart İçeriği -->
    <div class="card-body py-4">
        <!-- Form Wizard -->
        <div class="form-wizard">
            <!-- Adım Göstergeleri -->
            <div class="form-wizard-steps">
                <div class="form-wizard-progress">
                    <div class="form-wizard-progress-bar" style="width: 0%"></div>
                </div>

                <div class="form-wizard-step active" data-step="1">
                    <div class="form-wizard-step-icon">
                        <i class="bi bi-info-circle"></i>
                    </div>
                    <div class="form-wizard-step-label">Proje Bilgileri</div>
                </div>

                <div class="form-wizard-step" data-step="2">
                    <div class="form-wizard-step-icon">
                        <i class="bi bi-building"></i>
                    </div>
                    <div class="form-wizard-step-label">Sektör</div>
                </div>

                <div class="form-wizard-step" data-step="3">
                    <div class="form-wizard-step-icon">
                        <i class="bi bi-bar-chart"></i>
                    </div>
                    <div class="form-wizard-step-label">Metot</div>
                </div>

                <div class="form-wizard-step" data-step="4">
                    <div class="form-wizard-step-icon">
                        <i class="bi bi-people"></i>
                    </div>
                    <div class="form-wizard-step-label">Saha Firması</div>
                </div>

                <div class="form-wizard-step" data-step="5">
                    <div class="form-wizard-step-icon">
                        <i class="bi bi-calendar3"></i>
                    </div>
                    <div class="form-wizard-step-label">Zaman Planı</div>
                </div>

                <div class="form-wizard-step" data-step="6">
                    <div class="form-wizard-step-icon">
                        <i class="bi bi-person-workspace"></i>
                    </div>
                    <div class="form-wizard-step-label">Proje Ekibi</div>
                </div>

                <div class="form-wizard-step" data-step="7">
                    <div class="form-wizard-step-icon">
                        <i class="bi bi-check-circle"></i>
                    </div>
                    <div class="form-wizard-step-label">Tamamla</div>
                </div>
            </div>

            <!-- Form İçeriği -->
            <form id="kt_modal_create_app_form">
                <!-- Adım 1: Proje Bilgileri -->
                <div class="form-wizard-panel active" data-step="1">
                    <div class="mb-10">
                        <h2 class="fw-bold step-title">Proje Bilgileri</h2>
                        <p class="text-gray-600 fs-6">Projenin temel bilgilerini bu adımda giriniz. Doldurulması zorunlu alanlar * ile işaretlenmiştir.</p>
                    </div>

