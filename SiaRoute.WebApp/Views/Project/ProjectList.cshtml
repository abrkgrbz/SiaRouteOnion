    <script>
        document.addEventListener('DOMContentLoaded', (event) => {
            const otherOption = document.getElementById('other-option');
            const otherTextField = document.getElementById('other-text-field');
            const otherOptionMethod=document.getElementById('other-option-method');
            const otherOptionTextMethod=document.getElementById('other-option-method-field');

            otherOption.addEventListener('change', (event) => {
                if (event.target.checked) {
                    otherTextField.classList.remove('d-none');
                }
            });

            document.querySelectorAll('input[name="ProjectSector"]').forEach((input) => {
                if (input !== otherOption) {
                    input.addEventListener('change', (event) => {
                        if (event.target.checked) {
                            otherTextField.classList.add('d-none');
                        }
                    });
                }
            });

              otherOptionMethod.addEventListener('change', (event) => {
                
                if (event.target.checked) {
                    otherOptionTextMethod.classList.remove('d-none'); 
                }
            });

            document.querySelectorAll('input[name="ProjectMethod"]').forEach((input) => {
                if (input !== otherOptionMethod) {
                    input.addEventListener('change', (event) => {
                             if (event.target.checked) {
                input.classList.add('checked');
            } else {
                input.classList.remove('checked');
            }

            if (input !== otherOptionMethod) {
                otherOptionTextMethod.classList.add('d-none');
            }
                    });
                }
            });
        });
    </script>
    <script>
                       $(document).ready(function () {

                   $("#sahaSelect").select2();
                  $.ajax({
                    url: "@Url.Action("GetAllList", "Scm")",

                    method: "GET",
                    success: function (data) {
                        console.log(data)
                      data.forEach(function (firma) {
                        $("#sahaSelect").append(new Option(firma.scmName, firma.id));
                      });
                    },
                    error: function (xhr, status, error) {
                      console.error("Firma verileri yüklenirken hata oluştu:", error);
                    },
                  });
                });

                    $(document).ready(function () {
                  $("#userSelect").select2();
                  $("#userManagerSelect").select2();

                  $.ajax({
                    url: "@Url.Action("GetAllUserGroupByDepartman", "User")",

                    method: "GET",
                    success: function (data) {
                        console.log(data)
                      data.forEach(function (group) {
                        var optgroup = $('<optgroup>').attr('label', group.departmanName);
                       group.users.forEach(function(user)
                       {
                           optgroup.append(new Option(user.fullName, user.userId)); });
                            $('#userSelect').append(optgroup);
                            $('#userManagerSelect').append(optgroup.clone());
                      });

                    },
                    error: function (xhr, status, error) {
                      console.error("Kullanıcı verileri yüklenirken hata oluştu:", error);
                    },
                  });

                });
             function toggleInput(method) {
            const inputField = document.getElementById(`${method}-input`);
            if (inputField) {
                inputField.classList.toggle('d-none');
            }
                }

        document.addEventListener('DOMContentLoaded', function() {
            document.querySelectorAll('input[name="ProjectMethod"]:checked').forEach(el => {
                toggleInput(el.value);
            });
        });
                 
    </script>
}